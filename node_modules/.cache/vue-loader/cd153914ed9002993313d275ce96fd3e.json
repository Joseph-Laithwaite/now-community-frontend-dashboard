{"remainingRequest":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/src/pages/Dashboard/Pages/Register.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/src/pages/Dashboard/Pages/Register.vue","mtime":1598060882000},{"path":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/josephlaithwaite/Documents/VueLearning/vue-material-dashboard-laravel-master/vue-material-dashboard/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Register.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsGA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Register.vue","sourceRoot":"src/pages/Dashboard/Pages","sourcesContent":["<template>\n  <div class=\"md-layout\">\n    <div class=\"md-layout-item\">\n      <form @submit.prevent=\"register\">\n        <signup-card>\n          <h2 class=\"title text-center\" slot=\"title\">Register</h2>\n          <div\n            class=\"md-layout-item md-size-50 md-medium-size-50 md-small-size-100 ml-auto\"\n            slot=\"content-left\"\n          >\n            <div\n              class=\"info info-horizontal\"\n              v-for=\"item in contentLeft\"\n              :key=\"item.title\"\n            >\n              <div :class=\"`icon ${item.colorIcon}`\">\n                <md-icon>{{ item.icon }}</md-icon>\n              </div>\n              <div class=\"description\">\n                <h4 class=\"info-title\">{{ item.title }}</h4>\n                <p class=\"description\">\n                  {{ item.description }}\n                </p>\n              </div>\n            </div>\n          </div>\n          <div\n            class=\"md-layout-item md-size-50 md-medium-size-50 md-small-size-100 mr-auto\"\n            slot=\"content-right\"\n          >\n            <div class=\"social-line text-center\">\n              <md-button class=\"md-just-icon md-round md-twitter\">\n                <i class=\"fab fa-twitter\"></i>\n              </md-button>\n              <md-button class=\"md-just-icon md-round md-dribbble\">\n                <i class=\"fab fa-dribbble\"></i>\n              </md-button>\n              <md-button class=\"md-just-icon md-round md-facebook\">\n                <i class=\"fab fa-facebook-f\"></i>\n              </md-button>\n              <h4 class=\"mt-3\">or be classical</h4>\n            </div>\n\n            <md-field\n              class=\"md-form-group md-invalid\"\n              style=\"margin-bottom: 2rem\"\n            >\n              <md-icon>face</md-icon>\n              <label>Name</label>\n              <md-input v-model=\"name\" />\n              <validation-error :errors=\"apiValidationErrors.name\" />\n            </md-field>\n\n            <md-field\n              class=\"md-form-group md-invalid\"\n              style=\"margin-bottom: 2rem\"\n            >\n              <md-icon>email</md-icon>\n              <label>Email</label>\n              <md-input v-model=\"email\" />\n              <validation-error :errors=\"apiValidationErrors.email\" />\n            </md-field>\n\n            <md-field\n              class=\"md-form-group md-invalid\"\n              style=\"margin-bottom: 2rem\"\n            >\n              <md-icon>lock_outline</md-icon>\n              <label>Password</label>\n              <md-input v-model=\"password\" type=\"password\" />\n              <validation-error :errors=\"apiValidationErrors.password\" />\n            </md-field>\n\n            <md-field class=\"md-form-group md-invalid\">\n              <md-icon>lock_outline</md-icon>\n              <label>Confirm Password</label>\n              <md-input v-model=\"password_confirmation\" type=\"password\" />\n              <validation-error\n                :errors=\"apiValidationErrors.password_confirmation\"\n              />\n            </md-field>\n\n            <md-checkbox v-model=\"boolean\"\n              >I agree to the <a>terms and conditions</a>.</md-checkbox\n            >\n\n            <div class=\"button-container\">\n              <md-button\n                class=\"md-success md-round mt-4\"\n                type=\"submit\"\n                slot=\"footer\"\n              >\n                Get Started\n              </md-button>\n            </div>\n          </div>\n        </signup-card>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { SignupCard, ValidationError } from \"@/components\";\nimport formMixin from \"@/mixins/form-mixin\";\n\nexport default {\n  components: {\n    SignupCard,\n    ValidationError,\n  },\n  mixins: [formMixin],\n  data() {\n    return {\n      name: null,\n      boolean: false,\n      email: null,\n      role: 1,\n      password: null,\n      password_confirmation: null,\n      contentLeft: [\n        {\n          colorIcon: \"icon-success\",\n          icon: \"timeline\",\n          title: \"Marketing\",\n          description:\n            \"We've created the marketing campaign of the website. It was a very interesting collaboration.\",\n        },\n\n        {\n          colorIcon: \"icon-danger\",\n          icon: \"code\",\n          title: \"Fully Coded in HTML5\",\n          description:\n            \"We've developed the website with HTML5 and CSS3. The client has access to the code using GitHub.\",\n        },\n\n        {\n          colorIcon: \"icon-info\",\n          icon: \"group\",\n          title: \"Built Audience\",\n          description:\n            \"There is also a Fully Customizable CMS Admin Dashboard for this product.\",\n        },\n      ],\n    };\n  },\n\n  methods: {\n    async register() {\n      if (!this.boolean) {\n        await this.$store.dispatch(\n          \"alerts/error\",\n          \"You need to agree with our terms and conditions.\"\n        );\n        return;\n      }\n\n      const user = {\n        data: {\n          type: \"token\",\n          attributes: {\n            name: this.name,\n            email: this.email,\n            password: this.password,\n            password_confirmation: this.password_confirmation,\n          },\n        },\n      };\n\n      const requestOptions = {\n        headers: {\n          Accept: \"application/vnd.api+json\",\n          \"Content-Type\": \"application/vnd.api+json\",\n        },\n      };\n\n      try {\n        await this.$store.dispatch(\"register\", { user, requestOptions });\n        await this.$store.dispatch(\n          \"alerts/success\",\n          \"Successfully registered.\"\n        );\n      } catch (e) {\n        await this.$store.dispatch(\n          \"alerts/error\",\n          \"Oops, something went wrong\"\n        );\n        this.setApiValidation(e.response.data.errors);\n      }\n    },\n  },\n};\n</script>\n"]}]}