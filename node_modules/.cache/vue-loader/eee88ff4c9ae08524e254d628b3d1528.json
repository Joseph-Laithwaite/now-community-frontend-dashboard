lt=\"title + ' logo'\" />\n    </div>\n    <div class=\"user-info\">\n      <a\n        data-toggle=\"collapse\"\n        :aria-expanded=\"!isClosed\"\n        @click.stop=\"toggleMenu\"\n        @click.capture=\"clicked\"\n      >\n        <span>\n          {{ title }}\n          <b v-if=\"!singleIndependent\" class=\"caret\"></b>\n        </span>\n      </a>\n\n      <collapse-transition v-if=\"!singleIndependent\">\n        <div v-show=\"!isClosed\">\n          <ul class=\"nav\">\n            <slot>\n              <li>\n                  <div class=\"photo\">\n                    <img :src=\"logo\" :alt=\"title + ' logo'\" />\n                  </div>\n                  <div class=\"user-info\">\n                    <a>\n                      <span>\n                        {{ title }}\n                      </span>\n                    </a>\n                  </div>\n              </li>\n            </slot>\n          </ul>\n        </div>\n      </collapse-transition>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n\n  data() {\n    return {\n      singleIndependent: false,\n      isClosed: true,\n      independents: {},\n    };\n  },\n\n  async created() {\n    this.$store.watch(() => this.$store.getters[\"profile/myPermissions\"], (me) => {\n      \n      \n      //this.logo = process.env.VUE_APP_APP_BASE_URL + me.profile_picture\n    })\n    await this.$store.dispatch(\"profile/myPermissions\")\n  },\n\n  methods: {\n    clicked: function(e) {\n      e.preventDefault();\n    },\n    toggleMenu: function() {\n      this.isClosed = !this.isClosed;\n    },\n    goToProfile() {\n      this.$router.push({name: \"independent Profile\"})\n    },\n    logout() {\n      this.$store.dispatch(\"logout\");\n    }\n  }\n}\n</script>\n<style>\n.collapsed {\n  transition: opacity 1s;\n}\n</style>\n"]}]}