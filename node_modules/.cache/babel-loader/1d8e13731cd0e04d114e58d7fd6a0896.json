BC,QAAmB,SAAnBA,QAAmB;AAClC,WAAO,IAAIE,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACtCf,MAAAA,OAAO,CAACyB,OAAR,CAAgBb,MAAhB,EACCI,IADD,CACM,UAACZ,KAAD,EAAW;AACf,eAAOU,OAAO,CAACV,KAAD,CAAd;AACD,OAHD,EAICc,KAJD,CAIO,UAACC,KAAD,EAAS;AACd,eAAOJ,MAAM,CAACI,KAAD,CAAb;AACD,OAND;AAOD,KARM,CAAP;AASD;AA5Da,CAAhB;AA+DA,IAAMO,OAAO,GAAG;AACdxB,EAAAA,IAAI,EAAE,cAAAD,KAAK;AAAA,WAAIA,KAAK,CAACC,IAAV;AAAA,GADG;AAEdC,EAAAA,UAAU,EAAG,oBAAAF,KAAK;AAAA,WAAIA,KAAK,CAACE,UAAV;AAAA,GAFJ;AAGd;AACA;AACA;AACA;AACAC,EAAAA,KAAK,EAAE,eAAAH,KAAK;AAAA,WAAIA,KAAK,CAACG,KAAV;AAAA;AAPE,CAAhB;AAUA,IAAMuB,MAAM,GAAG;AACbC,EAAAA,UAAU,EAAE,IADC;AAEb3B,EAAAA,KAAK,EAALA,KAFa;AAGbyB,EAAAA,OAAO,EAAPA,OAHa;AAIbjB,EAAAA,OAAO,EAAPA,OAJa;AAKbJ,EAAAA,SAAS,EAATA;AALa,CAAf;AAQA,eAAesB,MAAf","sourcesContent":["import service from '@/store/services/brands-service';\n\nconst state = {\n  list: {},\n  pagination: {},\n  brand: {},\n};\n\nconst mutations = {\n  SET_LIST: (state, list) => {\n    state.list = list;\n  },\n  SET_RESOURCE: (state, brand) => {\n    state.brand = brand;\n  },\n  SET_PAGINATE: (state, pagination) => {\n    state.pagination = pagination;\n  }\n};\n\nconst actions = {\n  list({commit, dispatch}, params='') {\n    return new Promise((resolve, reject) => {\n    service.list(params)\n      .then(({list, meta, pagination}) => {\n        commit('SET_LIST', list);\n        commit('SET_PAGINATE', pagination);\n        return resolve(list);\n      })\n      .catch((error)=>{\n        console.log(\"brand Module error response\");\n        return reject(error);\n      });\n    })\n  },\n\n  get({commit, dispatch}, params='') {\n    return new Promise((resolve, reject) => {\n       service.get(params)\n      .then((brand) => {\n        commit('SET_RESOURCE', brand);\n        return resolve(brand);\n      })\n      .catch((error)=>{\n        console.log(\"brand Module error response\");\n        return reject(error);\n      });\n    })\n  },\n\n  add({commit, dispatch}, params) {\n    return service.add(params)\n      .then((brand) => { commit('SET_RESOURCE', brand); });\n  },\n\n  update({commit, dispatch}, params) {\n    return new Promise((resolve, reject) => {\n      service.update(params)\n      .then((brand) => { \n        console.log(\"brand Module success response\");\n        commit('SET_RESOURCE', brand);\n        return resolve(brand);\n      })\n      .catch((error)=>{\n        console.log(\"brand Module error response\");\n        return reject(error);\n      });\n    })\n  },\n\n  destroy({commit, dispatch}, params) {\n    return new Promise((resolve, reject) => {\n      service.destroy(params)\n      .then((brand) => { \n        return resolve(brand);\n      })\n      .catch((error)=>{\n        return reject(error);\n      });\n    })\n  }\n};\n\nconst getters = {\n  list: state => state.list,\n  pagination:  state => state.pagination,\n  // currentPage: state => state.meta.current_page,\n  // perPage: state => state.meta.per_page,\n  // listTotal: state => state.meta.total,\n  // pageCount: state => state.meta.to,\n  brand: state => state.brand,\n};\n\nconst brands = {\n  namespaced: true,\n  state,\n  getters,\n  actions,\n  mutations\n};\n\nexport default brands;\n"]}]}