AP;AAID;;AAED,eAAe;AACbK,EAAAA,IAAI,EAAJA,IADa;AAEbE,EAAAA,GAAG,EAAHA,GAFa;AAGbsB,EAAAA,GAAG,EAAHA,GAHa;AAIbO,EAAAA,MAAM,EAANA,MAJa;AAKbG,EAAAA,OAAO,EAAPA,OALa;AAMbC,EAAAA,MAAM,EAANA;AANa,CAAf","sourcesContent":["import qs from 'qs';\nimport axios from 'axios';\nimport Jsona from 'jsona';\n\naxios.interceptors.response.use(function (response) {\n  if (response.statusText !== 'OK') {\n      return Promise.reject(response);\n  }\n  return response;\n}, function (error) {\n  // Do something with response error\n  return Promise.reject(error);\n});\n\n\nconst url = process.env.VUE_APP_API_BASE_URL;\nconst jsona = new Jsona();\n\nfunction list(params) {\n  // const options = {\n  //   params: params,\n  //   paramsSerializer: function (params) {\n  //     return qs.stringify(params, {encode: false});\n  //   }\n  // };\n\n  return axios.get(`${url}/users` + params)\n    .then(response => {\n      return {\n        list: response.data.data,\n        meta: response.data,\n        pagination: {\n          current_page: response.data.current_page,\n          first_page_url: response.data.first_page_url,\n          from: response.data.from,\n          last_page: response.data.last_page,\n          last_page_url: response.data.last_page_url,\n          next_page_url: response.data.next_page_url,\n          path: response.data.path,\n          per_page: parseInt(response.data.per_page),\n          prev_page_url: response.data.prev_page_url,\n          to: response.data.to,\n          total: response.data.total\n        }\n      };\n    });\n}\n\nfunction get(id) {\n  const options = {\n    headers: {\n      'Accept': 'application/vnd.api+json',\n      'Content-Type': 'application/vnd.api+json',\n    }\n  };\n\n  return axios.get(`${url}/users/${id}`, options)\n    .then(response => {\n      let user = response.data;\n      delete user.links;\n      return user;\n    });\n}\n\nfunction add(user) {\n  // const payload = jsona.serialize({\n  //   stuff: user,\n  //   includeNames: null\n  // });\n\n  const options = {\n    headers: {\n      'Accept': 'application/vnd.api+json',\n      'Content-Type': 'application/vnd.api+json',\n    },\n  \n  };\n  \n  return new Promise((resolve, reject) => {\n    axios.post(`${url}/users`, user, options)\n      .then(response => {\n        console.log('CREATE NEW USER RESPONSE:')\n        console.log(response);\n        resolve(response.data)\n      }).catch(function (error) {\n        console.log(error);\n        if(error.status==201){\n          console.log('CREATED NEW USER RESPONSE with 201:')\n          console.log(error);\n          resolve(error.data);\n        }else{\n          console.log(error.response.data);\n          reject(error.response.data);\n        }\n      });\n    })\n}\n\nfunction update(user) {\n  user._method='PATCH';\n\n  const options = {\n    // _method: 'PATCH',\n    headers: {\n      'Accept': 'application/vnd.api+json',\n      'Content-Type': 'application/vnd.api+json',\n    }\n  };\n\n  return new Promise((resolve, reject) => {\n    axios.post(`${url}/users/${user.id}/edit`, user, options)\n      .then(response => {\n        console.log(response);\n        resolve(response.data)\n      }).catch(function (error) {\n        let e = error.response.data\n        console.log(error.response.data);\n        reject(e);\n      });\n    })\n}\n\nfunction destroy(id) {\n  const options = {\n    _method: 'DELETE',\n    headers: {\n      'Accept': 'application/vnd.api+json',\n      'Content-Type': 'application/vnd.api+json',\n    }\n  };\n  return new Promise((resolve, reject) => {\n    axios.post(`${url}/users/${id}`, options)\n    .then(response => {\n        console.log(response);\n        resolve(response.data)\n    })\n    .catch(function (error) {\n        let e = error.response.data\n        console.log(error.response.data);\n        reject(e);\n    });\n  });\n}\n\nfunction upload(user, image) {\n  const bodyFormData = new FormData();\n  bodyFormData.append('attachment', image);\n\n  return axios.post(`${url}/uploads/users/${user.id}/profile-image`, bodyFormData)\n    .then(response => {\n      return response.data.url;\n    });\n}\n\nexport default {\n  list,\n  get,\n  add,\n  update,\n  destroy,\n  upload\n};\n\n"]}]}