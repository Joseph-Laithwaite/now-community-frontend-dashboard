 await this.$store.dispatch(\"profile/me\")\n  },\n\n  methods: {\n    clicked: function(e) {\n      e.preventDefault();\n    },\n    toggleMenu: function() {\n      this.isClosed = !this.isClosed;\n    },\n    goToProfile() {\n      this.$router.push({name: \"User Profile\"})\n    },\n    logout() {\n      this.$store.dispatch(\"logout\");\n    }\n  }\n}\n</script>\n<style>\n.collapsed {\n  transition: opacity 1s;\n}\n</style>\n"],"sourceRoot":"src/pages/Dashboard/Layout/Extra"}]}