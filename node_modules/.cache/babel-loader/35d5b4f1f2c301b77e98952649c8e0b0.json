QAAA,KAAA,EAAA,IAFA;AAGA,QAAA,KAAA,EAAA,IAHA;AAIA,QAAA,QAAA,EAAA,IAJA;AAKA,QAAA,SAAA,EAAA,IALA;AAMA,QAAA,MAAA,EAAA;AANA;AAPA,KAAA;AAgBA,GApBA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AAAA;;AACA,UAAA,SAAA,GAAA,cAAA,GAAA,CADA,CAEA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,UAAA,GAAA,IAAA,SAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,QAAA,CAAA,QAAA;;AACA,YAAA,QAAA,CAAA,QAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,QAAA,CAAA,QAAA,CAAA,CAAA,CAAA;AACA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA,qDAAA;AACA,OATA;AAUA;AAdA;AArBA,CAAA","sourcesContent":["<template>\n    <form @submit.prevent=\"lookUpAdress\">\n        <md-field>\n            <label>Postcode</label>\n            <md-input \n                type=\"text\" \n                :required=\"true\"\n                id=\"postcode\"\n                name=\"postcode\"\n                autocomplete=\"on\"\n                v-model=\"postcode\"\n            ></md-input>\n        </md-field>\n        <md-field>\n            <label>Building name or number</label>\n            <md-input \n                type=\"text\"\n                :required=\"false\" \n                id=\"street-address\"\n                name=\"street-address\"\n                autocomplete=\"on\"\n                v-model=\"house\"\n                ></md-input>\n        </md-field>\n        <span class=\"md-layout-item md-size-100\">\n            <md-button class=\"md-primary\" type=submit>\n                Lookup your address\n            </md-button>\n            <md-button>\n                Or enter manually\n            </md-button>\n        </span>\n        <md-field>\n            <md-select v-if=\"addressOptions.length>1\">\n                <md-option \n                    v-for=\"{addressOption, index} in addressOptions\" \n                    :key=\"index\" \n                    :value=\"addressOption\" \n                    v-text=\"addressOption[0]\"\n                ></md-option>\n            </md-select>\n        </md-field>\n    </form>\n</template>\n\n<script>\nimport Axios from 'axios';\n\nconst api = process.env.ADDRESS_LOOKUP_API_KEY;\nconst url = process.env.ADDRESS_LOOKUP_BASE_URL;\n\nexport default {\n    name:'address-lookup',\n    \n    data(){\n        return{\n            postcode:null,\n            house:null,\n            addressOptions:[],\n            latitude: null,\n            longitude: null,\n\n            fullAddress:{\n                Line1:null,\n                Line2:null,\n                Line3:null,\n                Locality:null,\n                Town_City:null,\n                County:null,\n            }\n        }\n    },\n    methods:{\n        lookUpAdress(){\n            let lookupURL = \"?api-key=\" + api;\n            // return \"?perPagePaginate=\" + this.perPageSelected + '&page=' + this.currentSelectedPage + '&nameQuery=' + this.search\n            Axios.get(`${url}`+lookupURL)\n                .then(response=>{\n                    this.addressOptions=response.adresses;\n                    if(response.adresses.length==1){\n                        this.setAdress(response.adresses[0]);\n                    }\n                })\n                .catch(error=>{\n                    this.$store.dispatch(\"alerts/error\", 'Sorry no addresses found with the given combination');\n                })\n        }\n    }\n}\n</script>"],"sourceRoot":"src/components/InputBlocks"}]}